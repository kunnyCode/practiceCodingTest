    "s = list(map(int, input()))\n",
    "# print(s)\n",
    "\n",
    "result = 0\n",
    "\n",
    "for i in range(len(s)):\n",
    "    if i == 0\n",
    "        result = s[i]\n",
    "        continue\n",
    "    \n",
    "    if result == 0 or s[i] == 0:\n",
    "        result = result + s[i]\n",
    "    else:\n",
    "        result = result * s[i]\n",
    "        \n",
    "\n",
    "print\n"
   ]
